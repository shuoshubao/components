{
  "name": "Table",
  "desc": "配置化表格",
  "props": [
    {
      "name": "columns",
      "desc": "配置",
      "type": "ITableColumn[]",
      "optionalValue": "",
      "default": "[]",
      "version": ""
    },
    {
      "name": "dataSource",
      "desc": "数据源",
      "type": "object[]",
      "optionalValue": "",
      "default": "[]",
      "version": ""
    },
    {
      "name": "remoteConfig",
      "desc": "接口配置",
      "type": "IRemoteConfig",
      "optionalValue": "",
      "default": "null",
      "version": ""
    },
    {
      "name": "extraConfig",
      "desc": [
        "其他配置都放在这里",
        "showTotal: 在左上角展示总条数",
        "storageKey: 展示表头配置,存储展示状态的localStorage的唯一标识",
        "fullScreen: 全屏"
      ],
      "type": "<pre class='dynamic-table-render-code'><code>{\n  showTotal: boolean,\n  storageKey: string,\n  fullScreen: boolean,\n  disabledSort: (record, index) => boolean\n}</code></pre>",
      "optionalValue": "",
      "default": {
        "showTotal": false,
        "storageKey": "''",
        "fullScreen": false,
        "disabledSort": null
      },
      "version": ""
    },
    {
      "name": "draggable",
      "desc": "开启拖拽排序(拖拽整行)",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "prependHeader",
      "desc": "Table 左上角插槽",
      "type": "ReactNode",
      "optionalValue": "",
      "default": "null",
      "version": ""
    },
    {
      "name": "appendHeader",
      "desc": "Table 右上角插槽",
      "type": "ReactNode",
      "optionalValue": "",
      "default": "null",
      "version": ""
    },
    {
      "name": "renderItem",
      "desc": "List 组件所需的 renderItem",
      "type": "(item, index) => ReactNode || {}",
      "optionalValue": "",
      "default": "null",
      "version": ""
    },
    {
      "name": "listProps",
      "desc": "List 组件的其他属性(不包含 'dataSource', 'renderItem', 'pagination', 'loading', 'rowKey', 'size', 'loadMore')",
      "type": "object",
      "optionalValue": "",
      "default": "null",
      "version": ""
    }
  ],
  "events": [
    {
      "name": "onEditableCellSave",
      "desc": "编辑单元格时的回调",
      "signature": "",
      "args": "({ index, dataIndex, value, dataSource }): Promise<void>",
      "version": ""
    },
    {
      "name": "onDragSortEnd",
      "desc": "拖拽排序完成回调",
      "signature": "",
      "args": "{ dataSource, fromIndex, toIndex }",
      "version": ""
    }
  ],
  "methods": [
    {
      "name": "search",
      "desc": "触发表格的远端数据请求, 即调用 remoteConfig.fetch",
      "args": "params: 接口所需参数",
      "version": ""
    },
    {
      "name": "getDataSource",
      "desc": "获取数据 dataSource",
      "args": "",
      "version": ""
    },
    {
      "name": "handleRemoveRow",
      "desc": "删除一条数据",
      "args": "key: 要删除行的rowKey值",
      "version": ""
    },
    {
      "name": "handleMoveRow",
      "desc": "换行",
      "args": "fromIndex, toIndex",
      "version": ""
    }
  ],
  "columns": [
    {
      "name": "template",
      "desc": "模板的配置",
      "type": "{ tpl: '', ...props }",
      "optionalValue": "",
      "default": {
        "tpl": "text",
        "emptyText": "--"
      },
      "version": ""
    },
    {
      "name": "visible",
      "desc": "列的可见性",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "tooltip",
      "desc": "title 的提示文案",
      "type": "string | string[]",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "transform",
      "desc": "数据转换器, 接口数据的进一步处理",
      "type": "(text, record, index) => any",
      "optionalValue": "",
      "default": "null",
      "version": ""
    },
    {
      "name": "editable",
      "desc": "是否可编辑",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "rules",
      "desc": "校验规则",
      "type": "rules[]",
      "optionalValue": "",
      "default": "[]",
      "version": ""
    },
    {
      "name": "filters",
      "desc": "筛选项",
      "type": "{ label, value }[]",
      "optionalValue": "",
      "default": "[]",
      "version": ""
    },
    {
      "name": "filterMultiple",
      "desc": "筛选项-true: 多选; false: 单选",
      "type": "{ label, value }[]",
      "optionalValue": "",
      "default": "[]",
      "version": ""
    }
  ],
  "maintainer": "fangt11"
}
